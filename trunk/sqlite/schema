CREATE TABLE func (key INTEGER PRIMARY KEY, max INTEGER, min INTEGER, maxact INTEGER, minact INTEGER, value TEXT, pin INTEGER);
CREATE TABLE hum (key INTEGER PRIMARY KEY, value INTEGER);
CREATE TABLE hum_new (key INTEGER PRIMARY KEY, value FLOAT);
CREATE TABLE start (pin INTEGER PRIMARY KEY, act INTEGER );
CREATE TABLE temp (key INTEGER PRIMARY KEY, value INTEGER);
CREATE TABLE temp_new (key INTEGER PRIMARY KEY, value FLOAT);
CREATE TABLE time (key INTEGER PRIMARY KEY, date TEXT, pin TEXT, act INTEGER, status INTEGER);
